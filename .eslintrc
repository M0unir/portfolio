{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
  },
  "plugins": ["angular"],
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "strict": [0, "global"],
    "angular/controller_name": 0,
  }
}
